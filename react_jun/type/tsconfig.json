{
	"compilerOptions": {
		"outDir": "./dist",
		"target": "es5",
		"module": "esnext",
		"jsx": "react",
		"noImplicitAny": true,
		"allowSyntheticDefaultImports": true,
		"lib": [
			"dom",
			"DOM.Iterable",
			"esnext"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true
	},
	"include": [
		"src"
	]
}